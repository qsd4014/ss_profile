      },
      {
        "clash_mode": "global",
        "server": "cf"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "server": "block"
      }
    ],
    "final": "cf",
    "independent_cache": true,
    "reverse_mapping": true,
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    }
